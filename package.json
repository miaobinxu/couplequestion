{
  "name": "inSky",
  "main": "index.tsx",
  "version": "1.0.0",
  "scripts": {
    "start": "npx expo start",
    "android": "npx expo run:android",
    "ios": "npx expo run:ios",
    "checkDependencies": "npx expo install --check",
    "doctor": "npx expo-doctor",
    "prebuild": "cd scripts && chmod +x before-prebuild.sh && ./before-prebuild.sh && cd .. && DEBUG=* npx expo prebuild --clean && cd scripts && chmod +x after-prebuild.sh && ./after-prebuild.sh && cd ..",
    "updateAndroidVersionCodeAndName": "cd scripts && chmod +x update-versioning-android.sh && ./update-versioning-android.sh && cd ..",
    "updateIosVersionCodeAndName": "cd scripts && chmod +x update-versioning-ios.sh && ./update-versioning-ios.sh && cd ..",
    "overTheAirUpdates": "chmod +x over-the-air-updates.sh && ./over-the-air-updates.sh",
    "rollbackOverTheAirUpdates": "eas update:rollback",
    "EASDevelopmentBuild": "eas build --profile development --platform all",
    "EASProductionBuild": "eas build --profile production --platform all"
  },
  "dependencies": {
